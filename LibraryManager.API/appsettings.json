{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "LibraryDbContextConnection": "Server=localhost;User=sa;Password=Password@123;Database=Library;Integrated Security=True;Encrypt=False;Trusted_Connection=False;MultipleActiveResultSets=true"
  },
  "JwtInfo": {
    "Secret": "",
    "Issuer": "LibraryManager.API",
    "Audience": "LibraryManager",
    "Expires": 10
  }
}